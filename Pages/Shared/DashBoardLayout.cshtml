                    {
                        <li id="link2"><a asp-page="/CharityWorkersView/Storage"><i class="fa fa-home fa-fw"></i> المخازن </a></li>
                        <li id="link4"><a asp-page="/CharityWorkersView/Tasks"><i class="fa fa-clock-o fa-fw"></i> المهام </a></li>
                        <li id="link10"><a asp-page="/CharityWorkersView/NeedyMenu"><i class="fa fa-heart-o"></i>  المستفيدين </a></li>

                    }
                    else if (Context.Session.GetString("UserType") == "DE")
                    {
                         @*  <ul class="aside-list">

                        <li id="link1"><a asp-page="/CharityWorkersView/statistics"><i class="fa fa-bar-chart fa-fw"></i> الاحصائيات </a></li>
                        <li id="link2"><a asp-page="/CharityWorkersView/Storage"><i class="fa fa-home fa-fw"></i> المخازن </a></li>

                        <li id="link4"><a asp-page="/CharityWorkersView/Tasks"><i class="fa fa-clock-o fa-fw"></i> المهام </a></li>
                         <li id="link6"><a asp-page="/CharityWorkersView/Employee"><i class="fa fa-address-card-o"></i> الموظفون </a></li>
                        <li id="link9"><a asp-page="/CharityWorkersView/DonationsMenu"><i class="fas fa-handshake-o"></i>  المتبرعين </a></li>
                        <li id="link10"><a asp-page="/CharityWorkersView/NeedyMenu"><i class="fa fa-heart-o"></i>  المستفيدين </a></li>
                        <li id="link11"><a asp-page="/CharityWorkersView/Volunteers"><i class="fa fa-user-plus fa-fw"></i>  المتطوعيين </a></li>
                        <li id="link7"><a asp-page="/CharityWorkersView/about"><i class="fa fa-laptop fa-fw"></i> عن الشركة </a></li>
                        <li id="link8"><a asp-page="/CharityWorkersView/contactus"><i class="fa fa-comments fa-fw"></i> معلومات التواصل </a></li>
                        </ul>*@
                    }

                }

            </ul>
        </aside>


        <div class="page-body">
            <nav class="navbar navbar-default my-nav">
                <div class="container-fluid">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                        <ul class="nav navbar-nav navbar-left">
                            <li class="dropdown">
                                <a href="#" class=" btn-notifcation notifcation-active"><i class="fa fa-bell"></i></a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle btn-acc" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">أحمد الجندي <span class="caret"></span></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">الملف الشخصي</a></li>
                                    <li role="separator" class="divider"></li>
                                    <li><a href="#">الاشعارات</a></li>
                                    <li role="separator" class="divider"></li>
                                    <li><a asp-page="/Index" asp-page-handler="LogOut">تسجيل خروج</a></li>

                                </ul>
                            </li>
                        </ul>
                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container-fluid -->
