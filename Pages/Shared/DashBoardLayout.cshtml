                        <li id="link10"><a asp-page="/CharityWorkersView/NeedyMenu"><i class="fa fa-heart-o"></i>  المستفيدين </a></li>
                        <li id="link11"><a asp-page="/CharityWorkersView/Volunteers"><i class="fa fa-user-plus fa-fw"></i>  المتطوعيين </a></li>
                        <li id="link7"><a asp-page="/CharityWorkersView/about"><i class="fa fa-laptop fa-fw"></i> عن الشركة </a></li>
                        <li id="link8"><a asp-page="/CharityWorkersView/contactus"><i class="fa fa-comments fa-fw"></i> معلومات التواصل </a></li>
                    }
