        <script src="https://kit.fontawesome.com/49b2a74f1c.js" crossorigin="anonymous"></script>
    </head>



    <body>

        <aside class="my-side">
            <div class="logo-main">
                <img src="~/images/CharityWorkersView/main-logo.png">
            </div>
            <ul class="aside-list">
                @{
                    if (Context.Session.GetString("UserType") == "A")
                    {
                        <li id="link1"><a asp-page="/CharityWorkersView/statistics"><i class="fa fa-bar-chart fa-fw"></i> الاحصائيات </a></li>
                        <li id="link2"><a asp-page="/CharityWorkersView/Storage"><i class="fa fa-home fa-fw"></i> المخازن </a></li>
                        <li id="link4"><a asp-page="/CharityWorkersView/Tasks"><i class="fa fa-clock-o fa-fw"></i> المهام </a></li>
                        <li id="link6"><a asp-page="/CharityWorkersView/Employee"><i class="fa fa-address-card-o"></i> الموظفون </a></li>
